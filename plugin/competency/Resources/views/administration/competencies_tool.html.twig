{% extends 'ClarolineCoreBundle:administration:layout.html.twig' %}

{% block title %}
    {{ parent() ~ ' - ' ~ 'competencies'|trans({}, 'tools')|striptags|raw }}
{% endblock %}

{% import 'ClarolineCoreBundle::macros.html.twig' as macros %}

{% block breadcrumb %}
    {{ macros.breadcrumbs([
    {
        'name': 'administration'|trans({}, 'platform'),
        'href': path('claro_admin_index')
    },
    {
        'name': 'competencies'|trans({}, 'tools'),
        'href': ''
    }
    ]) }}
{% endblock %}

{% block content %}
    <div
        class="page-container competencies-container"
    >
    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    <script type="text/javascript" src="{{ hotAsset('dist/claroline-distribution-plugin-competency-competencies.js') }}"></script>
{% endblock %}
