BEGIN:VCALENDAR
{% for event in events %}
PRODID:-BayBuk
VERSION:2.0
CALSCALE:GREGORIAN
METHOD:PUBLISH
X-WR-CALNAME: {{ event.getUser().getUsername() }}
X-WR-TIMEZONE: {{ tzName }}
BEGIN:VEVENT
{% if event.getStart() %}DTSTART: {{ event.getStart().getTimestamp()|date('Ymd\\THi') }}00 {% endif %}

{% if event.getEnd() %}DTEND:{{ event.getEnd().getTimestamp()|date('Ymd\\THi') }}00 {% endif %}

CLASS:CLAROLINECONNECT
DESCRIPTION: {{ event.getDescription() }}
LOCATION:
STATUS:CONFIRMED
SUMMARY: {{ event.getTitle() }}
END:VEVENT
{% endfor %}
END:VCALENDAR
